{
  "name": "puppet-wireguard",
  "version": "4.1.0",
  "author": "Vox Pupuli",
  "summary": "installs, configures, and manages wireguard",
  "license": "AGPL-3.0",
  "source": "https://github.com/voxpupuli/puppet-wireguard.git",
  "project_page": "https://github.com/voxpupuli/puppet-wireguard",
  "issues_url": "https://github.com/voxpupuli/puppet-wireguard/issues",
  "description": "This is a nice module to manage wireguard backup jobs",
  "tags": [
    "wireguard",
    "vpn",
    "wg",
    "systemd-networkd"
  ],
  "dependencies": [
    {
